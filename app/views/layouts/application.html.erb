<!DOCTYPE html>
<html>
<head>
  <title>CovermynewsApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'layouts/navbar' %>
  <% flash.each do |name, msg| %>
     <%= content_tag(:div, msg, class: "alert alert-info center") %>
  <% end %>
  <div class='container' class='col-md-9 center-block'>
    <%= yield %>
  </div>
</body>
</html>
